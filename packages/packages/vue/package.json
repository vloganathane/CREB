{
  "name": "@creb/vue",
  "version": "1.0.0",
  "description": "Vue.js composables for CREB-JS chemistry calculations",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
